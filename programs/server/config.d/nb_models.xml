<clickhouse>
    <nb_models>
        <model>
            <name>sentiment</name>
            <path>/etc/clickhouse-server/config.d/nb_model_sentiment.txt</path>
            <priors>
                <prior>
                    <class>positive</class>
                    <value>0.6</value>
                </prior>
               <prior>
                    <class>negative</class>
                    <value>0.4</value>
                </prior>
            </priors>
        </model>
        <model>
            <name>language</name>
            <path>/etc/clickhouse-server/config.d/nb_model_language.txt</path>
        </model>
    </nb_models>
 </clickhouse>