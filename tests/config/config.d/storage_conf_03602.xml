<clickhouse>
    <storage_configuration>
        <disks>
            <azure_plain_rewritable_03602>
                <type>object_storage</type>
                <object_storage_type>azure</object_storage_type>
                <metadata_type>plain_rewritable</metadata_type>
                <skip_access_check>false</skip_access_check>
                <container_name>openbucketforpublicci</container_name>
                <connection_string from_env="AZURE_CONNECTION_STRING"/>
            </azure_plain_rewritable_03602>
            <cached_azure_plain_rewritable_03602>
                <type>cache</type>
                <disk>azure_plain_rewritable_03602</disk>
                <path>azure_cache/</path>
                <cache_policy>LRU</cache_policy>
                <max_size>100000000000</max_size>
                <cache_on_write_operations>1</cache_on_write_operations>
            </cached_azure_plain_rewritable_03602>
            <cached_azure_plain_rewritable_encrypted_03602>
                <type>encrypted</type>
                <disk>cached_azure_plain_rewritable_03602</disk>
                <key>1234567812345678</key>
            </cached_azure_plain_rewritable_encrypted_03602>
        </disks>
        <policies>
            <azure_plain_rewritable_encrypted_03602>
                <volumes>
                    <main>
                        <disk>cached_azure_plain_rewritable_encrypted_03602</disk>
                    </main>
                </volumes>
            </azure_plain_rewritable_encrypted_03602>
        </policies>
    </storage_configuration>
</clickhouse>
