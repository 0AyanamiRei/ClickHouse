<clickhouse>
    <storage_configuration>
        <policies>
            <nonencrypted>
                <volumes>
                    <main><disk>cached_azure_encrypted</disk></main>
                </volumes>
            </nonencrypted>
        </policies>
    </storage_configuration>
    <merge_tree>
        <storage_policy>azure</storage_policy>
    </merge_tree>
    <temporary_data_in_cache>cached_azure_encrypted</temporary_data_in_cache>
</clickhouse>
