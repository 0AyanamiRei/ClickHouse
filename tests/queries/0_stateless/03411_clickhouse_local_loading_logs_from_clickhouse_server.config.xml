<clickhouse>
    <logger>
        <level>trace</level>
        <console>true</console>
    </logger>

    <tcp_port>0</tcp_port>
    <path>.</path>
    <listen_host>127.0.0.1</listen_host>
    <mark_cache_size>0</mark_cache_size>
    <shutdown_wait_unfinished>0</shutdown_wait_unfinished>
    <users>
        <default>
            <password></password>

            <networks>
                <ip>127.0.0.1/32</ip>
            </networks>

            <profile>default</profile>
            <quota>default</quota>
        </default>
    </users>

    <profiles>
        <default/>
    </profiles>

    <quotas>
        <default />
    </quotas>

    <query_log />
</clickhouse>
