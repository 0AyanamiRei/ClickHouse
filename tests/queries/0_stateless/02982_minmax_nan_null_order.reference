-- Map with NULL
SELECT min(map(0, c1)), max(map(0, c1)) FROM values(NULL, 0, 5., 5.);
{0:0}	{0:5}
SELECT map(0, c1) as t FROM values(NULL, 0, 5., 5.) ORDER BY t ASC LIMIT 1;
{0:0}
SELECT map(0, c1) as t FROM values(NULL, 0, 5., 5.) ORDER BY t DESC LIMIT 1;
{0:5}
