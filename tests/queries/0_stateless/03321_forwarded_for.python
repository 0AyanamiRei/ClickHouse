#!/usr/bin/env python3
import os
import sys

CURDIR = os.path.dirname(os.path.realpath(__file__))
sys.path.insert(0, os.path.join(CURDIR, "helpers"))

from pure_http_client import ClickHouseClient

client = ClickHouseClient()

print(client.query("SELECT 1", headers={"X-Forwarded-For":"123.124.125.126"}))
print(client.query("SELECT 1", headers={"X-Forwarded-For":"123.124.125.126:80"}))
print(client.query("SELECT 1", headers={"X-Forwarded-For":"[::1.1.1.1]:80"}))
print(client.query("SELECT 1", headers={"X-Forwarded-For":"::1.1.1.1"}))

